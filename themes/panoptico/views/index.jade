  section#contact
    .container
      .row
        .col-lg-12.text-center
          h2.section-heading 
            i.fa.fa-comments-o
            |  #{__('IRC')}
          h3.section-subheading.text-muted #{__('#panopti.co @ freenode')}
          h2.section-heading 
            i.fa.fa-envelope-o
            |  #{__('Escribinos')}
          h3.section-subheading.text-muted #{__('Complete el formulario y nos pondremos en contacto a la brevedad.')}
      .row
        .col-lg-12
          form#contactForm(name='sentMessage' novalidate='' action="/contact")
            .row
              .col-md-6
                .form-group
                  input#name.form-control(type='text' placeholder=__('Su nombre')+'*' required='' data-validation-required-message=__('Ingrese su nombre')+'.')
                  p.help-block.text-danger
                .form-group
                  input#email.form-control(type='email' placeholder=__('Su email')+'*'  required='' data-validation-required-message=__('Ingrese su email')+'.')
                  p.help-block.text-danger
                .form-group
                  input#phone.form-control(type='tel' placeholder=__('Su teléfono')+'*' required='' data-validation-required-message=__('Ingrese su teléfono')+'.')
                  p.help-block.text-danger
              .col-md-6
                .form-group
                  textarea#message.form-control(placeholder=__('El mensaje')+'*' required='' data-validation-required-message=__('Ingrese su mensaje')+'.')
                  p.help-block.text-danger
              .clearfix
              .col-lg-12.text-center
                #success
                button.btn.btn-xl(type='submit') #{__('Enviar mensaje')}
