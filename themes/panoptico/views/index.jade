extends layout

block content
  // Navigation
  nav.navbar.navbar-default.navbar-fixed-top.bg-light-gray
    .container
      // Brand and toggle get grouped for better mobile display
      .navbar-header
        button.navbar-toggle(type='button' data-toggle='collapse' data-target='#bs-example-navbar-collapse-1')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand.page-scroll(href='#page-top') panopti.co
      // Collect the nav links, forms, and other content for toggling
      #bs-example-navbar-collapse-1.collapse.navbar-collapse
        ul.nav.navbar-nav.navbar-right
          li.hidden
            a(href='#page-top')
          li
            a.page-scroll(href='#wtf') #{__('(?)')}
          li.hidden
            a.page-scroll(href='#D') #{__(':D')}
          li
            a.page-scroll(href='#callfor2') #{__('Cf*')}
          li.hidden
            a.page-scroll(href='#staff') #{__('Equipo')}
          li
            a.page-scroll(href='#contact')
              i.fa.fa-envelope-o
      // /.navbar-collapse
    // /.container-fluid
  // Header
  header
    .container
      .intro-text
        .intro-heading
        .intro-lead-in
      a.page-scroll.btn.btn-xl.visible-md(href='#callto') #{__('Participar')}
  // Portfolio Section
  section#wtf(style="background:white;")
    .container
      .row
        .col-lg-12.text-center
          h2.section-heading #{__('(?)')}
          h3.section-subheading.text-muted #{__('panopti.co es una feria hecha con el objetivo de divertirnos observando y exponiendo proyectos, conocernos. Excusas para la cerveza sobran.')}
      .row
        .col-md-4.col-sm-6.portfolio-item
          a.portfolio-link(href='#feria-modal' data-toggle='modal')
            .portfolio-hover
              .portfolio-hover-content
                i.fa.fa-plus.fa-3x
            img.img-responsive(src='img/feria.jpg' alt='')
          .portfolio-caption
            h4 #{__('Feria')}
            p.text-muted #{__('Exposicion de proyectos y charlas relámpago')}
        .col-md-4.col-sm-6.portfolio-item
          a.portfolio-link(href='#talleres-modal' data-toggle='modal')
            .portfolio-hover
              .portfolio-hover-content
                i.fa.fa-plus.fa-3x
            img.img-responsive(src='img/talleres.jpg' alt='')
          .portfolio-caption
            h4 #{__('Talleres')}
            p.text-muted #{__('donde introducirse de forma práctica en nuevos conceptos')}
        .col-md-4.col-sm-6.portfolio-item
          a.portfolio-link(href='#working-area-modal' data-toggle='modal')
            .portfolio-hover
              .portfolio-hover-content
                i.fa.fa-plus.fa-3x
            img.img-responsive(src='img/working-area.jpg' alt='')
          .portfolio-caption
            h4 #{__('Area de trabajo')}
            p.text-muted #{__('Area con tranquilidad y comodidad para trabajar sin salir del evento.')}
  // Sponsors
  aside.clients
    .container
      .row
        h1.text-center #{__('Patrocinadores')}
        .col-md-3.col-sm-6
          a(href='#')
            img.img-responsive.img-centered(src='img/logos/milliwatt.png' alt='')
        .col-md-3.col-sm-6
          a(href='#')
            img.img-responsive.img-centered(src='img/logos/nirat.png' alt='')
  // Cf Seccion
  section#callfor2
    .container
      .row
        .col-lg-12.text-center
          h1 #{__('Llamado a feriantes y propuestas')}
          p.text-muted #{__('Podés proponer tu proyecto o idea, de eso se trata panopti.co, para ser considerada, esperamos contar con mucho espacio, así que no dudes en intentarlo ;).')}
          h3 #{__('El formulario para cargar propuestas será habilitado una vez agregado el segundo intercalar a junio.')}
  // About Section
  section.hidden#about
    .container
      .row
        .col-lg-12.text-center
          h2.section-heading #{__('Acerca de nosotros')}
          p.section-subheading.text-muted "#{__('We are legion. (?)')}"
      .row.hidden
        .col-lg-12
